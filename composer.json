{
    "name": "vanguardbackup/vanguard-php-sdk",
    "description": "The official Vanguard PHP SDK.",
    "keywords": ["vanguard", "backup", "sdk", "php"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/vanguardbackup/vanguard-php-sdk/issues",
        "source": "https://github.com/vanguardbackup/vanguard-php-sdk"
    },
    "authors": [
        {
            "name": "Lewis Larsen",
            "email": "lewis@larsens.dev"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1|^8.2|^8.3|^8.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "mockery/mockery": "^1.3.1",
        "phpstan/phpstan": "^1.10",
        "pestphp/pest": "^2.34",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "VanguardBackup\\Vanguard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VanguardBackup\\Vanguard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest",
        "pint": "pint",
        "pint:test": "pint --test"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}